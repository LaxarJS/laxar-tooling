   "widgetWithSchema": {
      "name": "widgetWithSchema",
      "definition": {
         "features": {
            "$schema": "http://json-schema.org/draft-04/schema#",
            "type": "object",
            "properties": {
               "button": {
                  "type": "object",
                  "required": ["action"],
                  "properties": {
                     "label": {
                        "type": ["string", "null"],
                        "default": "hit me"
                     },
                     "action": {
                        "type": "string",
                        "format": "topic"
                     }
                  }
               },
               "headline": {
                  "type": "object",
                  "properties": {
                     "enabled": {
                        "type": "boolean",
                        "default": false
                     }
                  }
               }
            }
         }
      }
   },
   "otherWidgetWithSchema": {
      "name": "otherWidgetWithSchema",
      "definition": {
         "features": {
            "$schema": "http://json-schema.org/draft-04/schema#",
            "type": "object",
            "properties": {
